{
  "name": "upload-to-firebase-storage",
  "version": "0.0.1",
  "description": "Uploads a file to firebase storage",
  "main": "index.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc --build ./tsconfig.json",
    "postbuild": "cpr package.json ./build/package.json && cd ./build && npm install --only=production",
    "start": "ts-node ./src/index.ts"
  },
  "author": "Peet Janse van Vuuren",
  "license": "MIT",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "firebase-admin": "^9.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "cpr": "^3.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.3"
  }
}
